{
  "name": "skanyxx-src",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:mac": "tsc -b && vite build && tauri build --target universal-apple-darwin",
    "build:windows": "tsc -b && vite build && tauri build --target x86_64-pc-windows-msvc",
    "build:linux": "tsc -b && vite build && tauri build --target x86_64-unknown-linux-gnu",
    "build:all": "npm run build:mac && npm run build:windows && npm run build:linux",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@tauri-apps/api": "^2.8.0",
    "@tauri-apps/plugin-http": "^2.5.2",
    "@tauri-apps/plugin-shell": "^2.3.0",
    "clsx": "^2.1.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.540.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
